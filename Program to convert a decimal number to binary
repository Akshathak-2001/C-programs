#include<stdio.h>
int main()
{
    int num,rem,i=0;
    int a[10];
    printf("Enter number:");
    scanf("%d",&num);
    while(num!=0)
    {
        rem=num%2;
        a[i++]=rem;
        num=num/2;
    } 
    printf("\nBinary equivalent:");
    for(i=i-1;i>=0;i--)
        printf("%d",a[i]);
    return 0;
}
